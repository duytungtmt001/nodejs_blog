<form class="mt-4" method="POST">
    <div class="from-group mb-3">
        <label for="exampleInputEmail1" class="form-label">Từ khóa</label>
        <input type="text" name="q" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Nhập từ khóa">
    </div>

    <div class="from-group mb-3">
        <label for="gender-input">Giới tính</label>
        <select name="gender" id="gender-input" class="form-control">
            <option value="">-- Chọn giới tính --</option>
            <option value="male">Nam</option>
            <option value="female">Nữ</option>
        </select>        
    </div>

    <button type="submit" class="btn btn-primary">Tìm kiếm</button>
</form>